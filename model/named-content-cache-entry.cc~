#include "ns3/type-id.h"
#include "ns3/type-name.h"
#include "named-content-cache-entry.h"

namespace ns3{
//namespace oicn{

NamedContentCacheEntry::NamedContentCacheEntry()
                      : m_data(),
                        m_ttl(0), 
                        m_timestamp(0), 
                        m_frequency(1), 
                        m_sourceip(0), 
                        m_packetsize(0),
                        m_id(0)

{

}

NamedContentCacheEntry::~NamedContentCacheEntry()
{

}

inline void NamedContentCacheEntry::SetData (uint8_t data);
{
  m_data = data;
}

inline uint8_t NamedContentCacheEntry::GetData ();
{
  return(m_data);
}
 
inline void NamedContentCacheEntry::SetTTL(uint32_t ttl);
{
  m_ttl = ttl;
}

inline uint32_t NamedContentCacheEntry::GetTTL();
{
  return(m_ttl);
}
 
inline void NamedContentCacheEntry::SetTimestamp(uint32_t timestamp);
{
  m_timestamp = timestamp;
}

inline uint32_t NamedContentCacheEntry::GetTimestamp()
{
  return(m_timestamp);
}

inline void NamedContentCacheEntry::SetFrequency(uint32_t frequency)
{
  m_frequency = frequency;
}

inline uint32_t NamedContentCacheEntry::GetFrequency()
{
  return(m_frequency);
}

inline void NamedContentCacheEntry::SetSourceIP(uint32_t sourceip)
{
  m_sourceip = sourceip;
}

inline uint32_t NamedContentCacheEntry::GetSourceIP()
{
  return(m_sourceip);
}

inline void NamedContentCacheEntry::SetPacketSize(uint32_t size)
{
  m_packetsize = size;
}

inline uint32_t NamedContentCacheEntry::GetPacketSize()
{
  return(m_packetsize);
}

inline void NamedContentCacheEntry::SetPacketID(uint32_t uid)
{
  m_id = uid;
}

inline uint32_t NamedContentCacheEntry::GetPacketID()
{
  return(m_id);
}
//} // end of namespace ns3
} // end of namespace oicn
